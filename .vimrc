syn on
set hls
set si
set ai
set tabstop=4
set bs=2
set shiftwidth=4
set expandtab
set wrap
set linebreak
set nolist  " list disables linebreak
set textwidth=0
set wrapmargin=0
set number
set clipboard=unnamed
set nu rnu

let &t_SI = "\<Esc>]50;CursorShape=1\x7"
let &t_SR = "\<Esc>]50;CursorShape=2\x7"
let &t_EI = "\<Esc>]50;CursorShape=0\x7"

"" so ~/.vim/plugins.vim
